{
  "manifest_version": 3,
  "name": "Clipboard URL Opener",
  "version": "1.1",
  "description": "",
  "permissions": ["tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "lookup_task": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Looks up task by ID"
    },

    "lookup_project": {
      "suggested_key": {
        "default": "Alt+K"
      },
      "description": "Looks up a project by ID"
    }
  },
  "host_permissions": ["<all_urls>"]
}
